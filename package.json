{
    "scripts": {
        "üîΩ css üîΩ": "",
        "css": "npm-run-all --print-label --silent --sequential css:sass css:postcss:tailwind css:postcss:autoprefixer",
        "css:dev": "npm-run-all --print-label --silent --sequential css:sass:dev css:postcss:tailwind",
        "css:watch": "onchange './client/_html/**/*.html' './client/_scss/**/*.scss' './client/**/*.php' --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"echo \\\"‚è≥ css:dev\\\" && npm run css:dev\" --title \"css:watch\" --message \"build failed\" --sound mute",
        "css:sass": "echo \"‚è≥\" && sass --style compressed --no-source-map ./client/_scss/style.scss ./$npm_package_config_build_folder/bundle.css && echo \"‚úÖ\"",
        "css:sass:dev": "echo \"‚è≥\" && sass ./client/_scss/style.scss ./$npm_package_config_build_folder/bundle.css && echo \"‚úÖ\"",
        "css:postcss:autoprefixer": "echo \"‚è≥\" && postcss --use autoprefixer --no-map --output ./$npm_package_config_build_folder/bundle.css ./$npm_package_config_build_folder/bundle.css && echo \"‚úÖ\"",
        "css:postcss:tailwind": "echo \"‚è≥\" && cross-env TAILWIND_MODE=build postcss --use @tailwindcss/postcss cssnano --no-map --no-autoprefixer --output ./$npm_package_config_build_folder/bundle.css ./$npm_package_config_build_folder/bundle.css && echo \"‚úÖ\"",
        "                                                                                                                                                                                                      ": "",
        "üîΩ js üîΩ": "",
        "js": "npm-run-all --print-label --silent --sequential js:vite js:minify",
        "js:dev": "npm-run-all --print-label --silent --sequential js:vite",
        "js:watch": "onchange './client/_js/**/*.js' './client/_libs/**/*.js' --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run js:dev\" --title \"js:watch\" --message \"build failed\" --sound mute",
        "js:vite": "echo \"‚è≥\" && vite build --logLevel error && echo \"‚úÖ\"",
        "js:minify": "echo \"‚è≥\" && terser --compress --mangle --comments false --output ./$npm_package_config_build_folder/bundle.js ./$npm_package_config_build_folder/bundle.js && echo \"‚úÖ\"",
        "                                                                                                                                                                                                       ": "",
        "üîΩ html üîΩ": "",
        "html": "npm-run-all --print-label --silent --sequential html:minify html:inline",
        "html:dev": "npm-run-all --print-label --silent --sequential html:minify:dev",
        "html:watch": "onchange './client/_html/**/*.html' --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run html:dev\" --title \"html:watch\" --message \"build failed\" --sound mute",
        "html:minify": "echo \"‚è≥\" && html-minifier-next --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --minify-css true --minify-js true --input-dir ./client/_html --output-dir ./$npm_package_config_build_folder --file-ext html && echo \"‚úÖ\"",
        "html:minify:dev": "echo \"‚è≥\" && html-minifier-next --input-dir ./client/_html --output-dir ./$npm_package_config_build_folder --file-ext html && echo \"‚úÖ\"",
        "html:inline": "echo \"‚è≥\" && inline-source --attribute \"data-inline\" --compress true --root ./$npm_package_config_build_folder ./$npm_package_config_build_folder/index.html > ./$npm_package_config_build_folder/index_inline.html && move-file ./$npm_package_config_build_folder/index_inline.html ./$npm_package_config_build_folder/index.html && echo \"‚úÖ\"",
        "                                                                                                                                                                                                        ": "",
        "üîΩ utils üîΩ": "",
        "copy": "echo \"‚è≥\" && ncp ./client/_assets ./$npm_package_config_build_folder/ && ncp ./client/_assets ./$npm_package_config_build_folder/ && echo \"‚úÖ\"",
        "copy:watch": "onchange './client/_assets/**/*.*' --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run copy\" --title \"copy:watch\" --message \"build failed\" --sound mute",
        "delete:files": "echo \"‚è≥\" && del-cli ./$npm_package_config_build_folder/*.* ./$npm_package_config_build_folder/**/ !./$npm_package_config_build_folder/index.html !./$npm_package_config_build_folder/favicon.png !./$npm_package_config_build_folder/_fonts/ && echo \"‚úÖ\"",
        "                                                                                                                                                                                                          ": "",
        "üîΩ pipelines üîΩ": "",
        "prod:‚ö°": "cross-env NODE_ENV=production npm-run-all --print-label --silent --sequential copy css js html delete:files",
        "dev:‚ö°": "cross-env NODE_ENV=development npm-run-all --print-label --silent --parallel copy:watch css:watch js:watch html:watch",
        "prod": "npm run prod:‚ö° -s",
        "dev": "npm run dev:‚ö° -s"
    },
